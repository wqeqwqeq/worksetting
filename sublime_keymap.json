    { "keys": ["ctrl+e", "ctrl+l"], "command": "st_list_queries", "args": {"mode" : "run"}},
    { "keys": ["ctrl+e", "ctrl+o"], "command": "st_list_queries", "args": {"mode" : "open"}},
    {"command": "st_display_func_doc", "keys": ["ctrl+alt+d"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    {"command": "st_select_current_query", "keys": ["ctrl+q"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    {"command": "st_add_subquery_wrap", "keys": ["ctrl+e", "ctrl+w"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    {"command": "st_reconnect_impala", "keys": ["ctrl+e", "ctrl+r"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    {"command": "st_interrupt_impala_execution", "keys": ["ctrl+e", "ctrl+i"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    {"command": "st_go_to_select", "keys": ["ctrl+e", "ctrl+g"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    {"command": "st_refresh_impala_meta", "keys": ["ctrl+e", "ctrl+m"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    {"command": "st_clear_result_cache", "keys": ["ctrl+e", "ctrl+c"], "context": [
            {"key": "selector", "operator": "equal", "operand": "source.sql"}
        ]},
    { "keys": ["shift+tab"], "command": "unindent", "context":
        [
            { "key": "preceding_text", "operator": "regex_match", "operand": "^[\t ]*" }
        ]
    },
    { "keys": ["shift+tab"], "command": "unindent", "context":
        [
            { "key": "text", "operator": "regex_contains", "operand": "\n" }
        ]
    },
    { "keys": ["shift+tab"], "command": "prev_field", "context":
        [
            { "key": "has_prev_field", "operator": "equal", "operand": true }
        ]
    },

    { "keys": ["alt+`"], "command": "toggle_terminus_panel" },  
    { "keys": ["ctrl+enter"], "command": "repl_transfer_current", "args": {"scope": "selection"}},
    { "keys": ["ctrl+shift+enter"], "command": "repl_transfer_current", "args": {"scope": "file"}},
    { "keys": ["ctrl+alt+p"], "command": "prompt_select_workspace" },
    { "keys": ["ctrl+1"], "command": "duplicate_line" },
    { "keys": ["ctrl+2"], "command": "upper_case" },
    { "keys": ["ctrl+3"], "command": "lower_case" },
    { "keys": ["ctrl+4"], "command": "save_workspace_as" },
    { "keys": ["ctrl+d"], "command": "run_macro_file", "args": {"file": "res://Packages/Default/Delete Line.sublime-macro"} },


